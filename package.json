{
  "name": "React-Sight",
  "version": "1.0.5",
  "description": "Visualization tool for React Applications",
  "keywords": [
    "react",
    "fiber",
    "redux",
    "router",
    "chrome",
    "extension",
    "developer tool",
    "dev tool"
  ],
  "scripts": {
    "webpack": "webpack -w",
    "test": "NODE_PATH=. node_modules/.bin/jest --verbose --coverage chrome-ext --collectCoverageFrom=**/chrome-ext/**/*.js",
    "coveralls": "NODE_PATH=. node_modules/.bin/jest --verbose --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "start": "npm-run-all -p webpack",
    "build": "webpack -p"
  },
  "repository": "git+https://github.com/React-Sight/React-Sight.git",
  "author": "David Sally, Grant Kang, & William He",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/React-Sight/React-Sight/issues"
  },
  "homepage": "https://github.com/React-Sight/React-Sight#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.0.4",
    "npm-run-all": "^4.1.1",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "d3": "^4.10.2",
    "json-formatter-js": "^2.2.0"
  }
}
