{
  "name": "react-sight",
  "version": "1.0.6",
  "description": "Visualization tool for React Applications",
  "keywords": [
    "react",
    "fiber",
    "redux",
    "router",
    "chrome",
    "extension",
    "developer tool",
    "dev tool"
  ],
  "scripts": {
    "start": "webpack -w",
    "start:dev": "NODE_PATH=. NODE_ENV='development' webpack-dev-server --content-base chrome-ext/build",
    "test": "NODE_PATH=. node_modules/.bin/jest",
    "coveralls": "NODE_PATH=. node_modules/.bin/jest && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "webpack -p",
    "build:debug": "NODE_ENV=debug webpack -w"
  },
  "repository": "git+https://github.com/React-Sight/React-Sight.git",
  "author": "David Sally, Grant Kang, & William He",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/React-Sight/React-Sight/issues"
  },
  "homepage": "https://github.com/React-Sight/React-Sight#readme",
  "dependencies": {
    "d3": "5.9.7",
    "d3-interpolate": "1.3.2",
    "json-formatter-js": "2.2.1",
    "lodash": "4.17.14"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/preset-env": "7.5.4",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.0.3",
    "coveralls": "3.0.0",
    "css-loader": "3.0.0",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "4.0.0",
    "image-webpack-loader": "5.0.0",
    "jest": "24.8.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
