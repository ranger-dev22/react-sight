{
  "name": "react-sight",
  "version": "1.0.6",
  "description": "Visualization tool for React Applications",
  "keywords": [
    "react",
    "fiber",
    "redux",
    "router",
    "chrome",
    "extension",
    "developer tool",
    "dev tool"
  ],
  "scripts": {
    "start": "webpack -w",
    "start:dev": "NODE_PATH=. NODE_ENV='development' webpack-dev-server --content-base chrome-ext/build",
    "test": "NODE_PATH=. node_modules/.bin/jest",
    "coveralls": "NODE_PATH=. node_modules/.bin/jest && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "webpack -p",
    "build:debug": "NODE_ENV=debug webpack -w"
  },
  "repository": "git+https://github.com/React-Sight/React-Sight.git",
  "author": "David Sally, Grant Kang, & William He",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/React-Sight/React-Sight/issues"
  },
  "homepage": "https://github.com/React-Sight/React-Sight#readme",
  "dependencies": {
    "d3": "5.0.0",
    "json-formatter-js": "2.2.0",
    "lodash": "4.17.5"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.3.1",
    "coveralls": "3.0.0",
    "css-loader": "0.28.9",
    "eslint": "4.16.0",
    "eslint-config-airbnb": "16.0.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.4.0",
    "file-loader": "1.1.6",
    "image-webpack-loader": "3.4.2",
    "jest": "23.6.0",
    "node-sass": "4.7.2",
    "npm-run-all": "4.1.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.11.1"
  }
}
